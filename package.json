{
  "name": "meki",
  "version": "1.0.0",
  "main": "main.js",
  "scripts": {
    "start": "electron .",

    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --no-asar --icon=icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . mk-stock --overwrite --no-asar --platform=win32 --arch=ia32 --icon=icon.ico --prune=true --out=mk-stock-exe --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Electron Tutorial App\"",
    "package-linux": "electron-packager . mk-stock --overwrite --no-asar --platform=linux --arch=x64 --icon=icon.png --prune=true --out=release-builds"
  },
  "author": "Meki Studio",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron-packager": "^15.5.1"
  },
  "dependencies": {
    "php-server-manager": "^1.2.1",
    "electron": "^17.1.0",
    "electron-packager": "^15.5.1"


  }
}
